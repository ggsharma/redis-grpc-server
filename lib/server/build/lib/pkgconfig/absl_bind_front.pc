prefix=/usr/local
exec_prefix=${prefix}
libdir=/usr/local/lib
includedir=/usr/local/include

Name: absl_bind_front
Description: Abseil bind_front library
URL: https://abseil.io/
Version: 20230802
Requires: absl_base_internal = 20230802, absl_compressed_tuple = 20230802
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
